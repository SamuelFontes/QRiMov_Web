@model Imovel
<div class="col-sm-4 col-lg-4 col-md-4">
    <div class="thumbnail">
        <img src="@Model.ImgUrl" class="img-rounded" alt="" />
    </div>
    <div class="caption">
        <h3 class="pull-right">Valor : @Model.Valor.ToString("c")</h3>
        <h3>Imóvel : @Model.Descricao</h3>
        <p>Valor : @Model.Valor.ToString("c")</p>
    </div>
</div>